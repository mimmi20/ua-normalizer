{
  "name": "mimmi20/ua-normalizer",
  "type": "library",
  "description": "the Normalizers extracted from the UaNormalizer PHP Library for PHP 7+",
  "keywords": [
    "UaNormalizer",
    "Browser",
    "user agent",
    "http",
    "parser",
    "user-agent"
  ],
  "homepage": "https://github.com/mimmi20/ua-normalizer",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/ua-normalizer/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16.0",
    "infection/infection": "^0.13.6",
    "localheinz/composer-normalize": "^1.3.1",
    "localheinz/phpstan-rules": "^0.13.0",
    "pepakriz/phpstan-exception-rules": "^0.9.0",
    "phpstan/phpstan": "^0.11.19",
    "phpstan/phpstan-deprecation-rules": "^0.11.2",
    "phpstan/phpstan-phpunit": "^0.11.2",
    "phpstan/phpstan-strict-rules": "^0.11.1",
    "phpunit/phpunit": "^7.5.17 || ^8.4.0",
    "squizlabs/php_codesniffer": "^3.5.2"
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "UaNormalizer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UaNormalizerTest\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/mimmi20/ua-normalizer/issues",
    "source": "https://github.com/mimmi20/ua-normalizer"
  }
}
