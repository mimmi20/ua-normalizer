{
  "name": "mimmi20/ua-normalizer",
  "type": "library",
  "description": "the Normalizers extracted from the UaNormalizer PHP Library for PHP 7+",
  "keywords": [
    "UaNormalizer",
    "Browser",
    "user agent",
    "http",
    "parser",
    "user-agent"
  ],
  "homepage": "https://github.com/mimmi20/ua-normalizer",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/ua-normalizer/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.4.0 || ^8.0.0",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "infection/infection": "^0.23.0",
    "mimmi20/coding-standard": "^2.2.2",
    "pepakriz/phpstan-exception-rules": "^0.11.7",
    "phpstan/extension-installer": "^1.1.0",
    "phpstan/phpstan": "^0.12.86",
    "phpstan/phpstan-deprecation-rules": "^0.12.6",
    "phpstan/phpstan-phpunit": "^0.12.18",
    "phpunit/phpunit": "^9.5.4"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "UaNormalizer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UaNormalizerTest\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/mimmi20/ua-normalizer/issues",
    "source": "https://github.com/mimmi20/ua-normalizer"
  }
}
