{
  "name": "mimmi20/ua-normalizer",
  "type": "library",
  "description": "the Normalizers extracted from the UaNormalizer PHP Library for PHP 7+",
  "keywords": [
    "UaNormalizer",
    "Browser",
    "user agent",
    "http",
    "parser",
    "user-agent"
  ],
  "homepage": "https://github.com/mimmi20/ua-normalizer",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/ua-normalizer/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.1.3 || ^8.0.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "~2.6.1",
    "ergebnis/phpstan-rules": "~0.15.1",
    "infection/infection": "~0.13.6 || ~0.16.4",
    "mimmi20/coding-standard": "~1.0.2",
    "pepakriz/phpstan-exception-rules": "~0.11.1",
    "phpstan/extension-installer": "~1.0.4",
    "phpstan/phpstan": "~0.12.37",
    "phpstan/phpstan-deprecation-rules": "~0.12.5",
    "phpstan/phpstan-phpunit": "~0.12.16",
    "phpstan/phpstan-strict-rules": "~0.12.4",
    "phpunit/phpunit": "~7.5.20 || ~8.5.8 || ~9.3.5"
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "UaNormalizer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UaNormalizerTest\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/mimmi20/ua-normalizer/issues",
    "source": "https://github.com/mimmi20/ua-normalizer"
  }
}
