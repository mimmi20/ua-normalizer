{
  "name": "mimmi20/ua-normalizer",
  "description": "the Normalizers extracted from the UaNormalizer PHP Library for PHP 5.3+",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "UaNormalizer",
    "Browser",
    "user agent",
    "http",
    "parser",
    "user-agent"
  ],
  "homepage": "https://github.com/mimmi20/ua-normalizer",
  "require": {
    "php": ">=5.3.3",
    "mimmi20/wurfl-constants": "^1.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8 || ^5.0",
    "squizlabs/php_codesniffer": "^2.0",
    "friendsofphp/php-cs-fixer": "^1.11"
  },
  "autoload": {
    "psr-4": {
      "UaNormalizer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UaNormalizerTest\\": "tests/"
    }
  }
}
